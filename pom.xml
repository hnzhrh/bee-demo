            <dependency>
                <groupId>com.hnzhrh</groupId>
                <artifactId>api</artifactId>
                <version>${bee-demo.version}</version>
            </dependency>
            <dependency>
                <groupId>com.hnzhrh</groupId>
                <artifactId>service</artifactId>
                <version>${bee-demo.version}</version>
            </dependency>
            <dependency>
                <groupId>com.hnzhrh</groupId>
                <artifactId>domain</artifactId>
                <version>${bee-demo.version}</version>
            </dependency>
        </dependencies>
