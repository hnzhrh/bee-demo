            <artifactId>bee-knife4j-spring-boot-starter</artifactId>
        </dependency>
    </dependencies>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.hnzhrh</groupId>
                <artifactId>web</artifactId>
                <version>${bee-demo.version}</version>
